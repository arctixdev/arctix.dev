{
	"name": "un10ck3d.github.io",
	"version": "0.0.1",
	"private": true,
	"postbuild": "npx svelte-sitemap --domain https://jonathanbangert.com --out-dir docs --reset-time --change-freq daily",
	"scripts": {
		"dev": "npx vite dev",
		"postbuild": "npx svelte-sitemap --domain https://jonathanbangert.com --out-dir docs --reset-time --change-freq daily",
		"build": "npx vite build && touch docs/.nojekyll && echo 'jonathanbangert.com' > docs/CNAME",
		"preview": "npx vite preview",
		"test": "npx jest --passWithNoTests",
		"check": "npx svelte-kit sync && npx svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "npx svelte-kit sync && npx svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "npx prettier --plugin-search-dir . --check . && npx eslint .",
		"format": "npx prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-auto": "^1.0.3",
		"@sveltejs/adapter-static": "^1.0.6",
		"@sveltejs/kit": "^1.15.2",
		"@tailwindcss/typography": "^0.5.9",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@types/jest": "^29.4.0",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"autoprefixer": "^10.4.13",
		"daisyui": "^2.50.0",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jest": "^29.4.2",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^2.10.3",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^5.0.1",
		"svelte-sitemap": "^2.5.4",
		"svelte-typewriter": "^3.1.4",
		"tailwindcss": "^3.2.6",
		"ts-jest": "^29.0.5",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.2.3"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/roboto": "^4.5.8",
		"bootstrap-icons": "^1.10.3",
		"jest-environment-jsdom": "^29.4.2",
		"svelte-meta-tags": "^2.6.5",
		"theme-change": "^2.3.0"
	}
}
